    "license": "MIT",
    "scripts": {
        "build": "nest build && email build",
        "format": "prettier --write \"src/**/*.{ts,json}\"",
        "start": "nest start",
        "start:dev": "dotenv -e .env.development -- nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "dotenv -e .env.production -- node dist/src/main",
        "lint": "eslint \"src/**/*.ts\" --fix",
        "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
        "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
        "prisma:push": "dotenv -e .env.development -- pnpm prisma db push",
        "prisma:studio": "dotenv -e .env.development -- pnpm prisma studio",
        "email": "dotenv -e .env.development -- email dev",
        "prepare": "husky"
    },
    "dependencies": {
        "@nest-lab/throttler-storage-redis": "^1.1.0",
        "@nestjs-modules/mailer": "^2.0.2",
        "@nestjs/bullmq": "^11.0.2",
