        "@nestjs/config": "^4.0.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/serve-static": "^5.0.3",
        "@nestjs/throttler": "^6.4.0",
        "@prisma/client": "^6.4.1",
        "@react-email/components": "0.0.33",
        "app-root-path": "^3.1.0",
        "argon2": "^0.41.1",
        "bullmq": "^5.41.7",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "compression": "^1.8.0",
        "cookie-parser": "^1.4.7",
        "dotenv-cli": "^8.0.0",
        "fs-extra": "^11.3.0",
        "helmet": "^8.0.0",
        "ioredis": "^5.5.0",
        "joi": "^17.13.3",
        "nestjs-i18n": "^10.5.0",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "prisma": "^6.4.1",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1"
    },
